{
  "name": "gradientAI-scrape",
  "version": "1.0.0",
  "description": "Library untuk berinteraksi dengan API Gradient AI (chat.gradient.network)",
  "main": "src/index.js",
  "scripts": {
    "release": "semantic-release",
    "commit": "cz"
  },
  "keywords": [],
  "author": "dhulfahmiismail123",
  "license": "ISC",
  "packageManager": "pnpm@10.20.0",
  "type": "module",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "semantic-release": "^24.0.0",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/release-notes-generator": "^15.0.0",
    "@semantic-release/changelog": "^7.0.0",
    "@semantic-release/github": "^10.3.0",
    "@semantic-release/git": "^10.0.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0"
  },
  "dependencies": {
    "impit": "^0.6.1"
  },
  "release": {
    "branches": ["main"],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/git"
    ],
    "tagFormat": "v${version}",
    "prepare": [
      "@semantic-release/changelog",
      {
        "path": "@semantic-release/git",
        "assets": ["CHANGELOG.md", "package.json"],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  }
}